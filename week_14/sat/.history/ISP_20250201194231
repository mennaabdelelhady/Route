<?php

interface PaymentMethod
{
    public function Credit();

    public function Fawry();

    public function PayPal();
}

class SystemTech implements PaymentMethod
{
    public function Credit()
    {
        return "Credit";
    }

    public function Fawry()
    {
        return "Fawry";
    }

    public function PayPal()
    {
        return "PayPal";
    }
}

class Tech implements PaymentMethod
{
    public function Credit()
    {
        return "Credit";
    }

    public function Fawry()
    {
    }

    public function PayPal()
    {
        return "PayPal";
    }
}